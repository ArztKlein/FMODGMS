        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Draw GUI
{
    draw_set_font(fnt_normal);
    draw_set_halign(fa_left);

    // CPU Usage
    draw_set_color(make_color_hsv((hue+15 mod 256),128,255));
    draw_text(16,32,"CPU Usage: ");
    
    draw_set_halign(fa_center);
    draw_set_color(make_color_hsv((hue mod 256),128,255));
    draw_text(210,16,string(FMODGMS_Sys_Get_CPUUsage()) + "%");
    
    draw_set_halign(fa_left);
    draw_healthbar(160,32,260,48,FMODGMS_Sys_Get_CPUUsage(),make_color_rgb(64,64,64),make_color_hsv((hue+42 mod 256),0,255),make_color_hsv((hue+42 mod 256),255,255),0,true,true);
    
    // FPS
    draw_set_color(make_color_hsv((hue+15 mod 256),128,255));
    draw_text(320,32,"FPS: " + string(fps) + "/" + string(room_speed));
    
    // Sound info
    draw_set_color(make_color_hsv((hue+45 mod 256),128,255));
    draw_text(16,64,"Audio Type: " + string(FMODGMS_Snd_Get_Type(sound[sound_index])));
    draw_set_color(make_color_hsv((hue+60 mod 256),128,255));
    draw_text(16,112,"Loop Points: " + string(FMODGMS_Snd_Get_LoopPoints(sound[sound_index],0)) + " - " + string(FMODGMS_Snd_Get_LoopPoints(sound[sound_index],1)));
    draw_set_color(make_color_hsv((hue+75 mod 256),128,255));
    draw_text(16,128,"Position: " + string(FMODGMS_Chan_Get_Position(0)));
    draw_set_color(make_color_hsv((hue+90 mod 256),128,255));
    draw_text(16,144,"Order: " + string(FMODGMS_Chan_Get_ModOrder(0))); // only works with module files
    draw_set_color(make_color_hsv((hue+105 mod 256),128,255));
    draw_text(16,160,"Row: " + string(FMODGMS_Chan_Get_ModRow(0))); // only works with module files
    
    draw_set_color(c_white);
    draw_text(16,240,"Space -- Play/Stop#1-5 -- Switch track#R -- Toggle fullscreen#Esc -- Quit");
    
    draw_set_color(make_color_hsv((hue+120 mod 256),128,255));
    draw_text(16,416,"FMODGMS Test Program - by M.S.T.O.P.");
    draw_set_color(make_color_hsv((hue+135 mod 256),128,255));
    draw_text(16,432,"FMODGMS version: 0.3.1");
    draw_set_color(make_color_hsv((hue+150 mod 256),128,255));
    draw_text(16,448,"FMOD version: Studio 1.07.06");
    
}
